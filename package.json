{
  "name": "kanolab-vanguard",
  "displayName": "Kanolab Vanguard",
  "description": "kanolab-vanguard",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nharu-0630/kanolab-vanguard.git"
  },
  "publisher": "nharu0630",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "kanolab-vanguard.start",
        "title": "Start Kanolab Vanguard"
      },
      {
        "command": "kanolab-vanguard.stop",
        "title": "Stop Kanolab Vanguard"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "dependencies": {
    "@types/git-diff": "^2.0.7",
    "@types/proper-lockfile": "^4.1.4",
    "git-diff": "^2.0.6",
    "proper-lockfile": "^4.1.2",
    "sha3": "^2.1.4"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "~20.17.30",
    "@types/vscode": "^1.99.1",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.24.0",
    "typescript": "^5.8.3"
  }
}